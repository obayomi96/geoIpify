{
  "version": "1.2.7",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": "*"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "npx jest && npm run test:coverage",
    "test:coverage": "jest --coverage",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run test && npm run build"
    }
  },
  "precommit": [
    "lint",
    "test",
    "build"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/obayomi96/geoIpify.git"
  },
  "keywords": [
    "network",
    "ip address",
    "isp",
    "node",
    "api"
  ],
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "geo-ipify",
  "author": "obayomi96",
  "type": "module",
  "module": "dist/geo-ipify.esm.js",
  "size-limit": [
    {
      "path": "dist/geo-ipify.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/geo-ipify.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^11.0.1",
    "@types/jest": "^29.5.14",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "pre-commit": "^1.2.2",
    "size-limit": "^11.0.1",
    "ts-jest": "^29.3.2",
    "tsdx": "^0.14.1",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "axios": "^1.6.3"
  }
}
